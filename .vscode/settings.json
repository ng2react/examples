{
  "ng2react.enabled": "yes",
  "ng2react.sandbox": false,
  "ng2react.openai.model": "gpt-4",
  "ng2react.source.angularRoot": "src/angular",
  "ng2react.source.reactRoot": "src/react",
  "ng2react.source.testRoot": "src/test",
  "cucumber.features": ["src/test/**/*.feature", "features/**/*.feature", "tests/**/*.feature", "*specs*/**/.feature"],
  "[feature]": {
    "editor.defaultFormatter": "alexkrechik.cucumberautocomplete"
  },
  "ng2react.customPrompt.location": ".ng2react",
  "ng2react.customPrompt.enabled": false
}
